#: includes/class-wc-enviofacil-shipping.php:19
#: includes/class-wc-enviofacil-shipping.php:22
msgid "Envio Fácil"
msgstr "Envio Fácil"

#: includes/class-wc-enviofacil-shipping.php:20
msgid "Adds shipping method PAC and SEDEX via Envio Fácil to WooCommerce."
msgstr "Adiciona os métodos de envio PAC e SEDEX via Envio Fácil ao WooCommerce."

#: includes/class-wc-enviofacil-shipping.php:45
msgid "Origin postcode"
msgstr "CEP do remetente"

#: includes/class-wc-enviofacil-shipping.php:47
msgid "It is the postcode from the sender."
msgstr "É o CEP de origem do pacote."

#: includes/class-wc-enviofacil-shipping.php:52
msgid "Estimated delivery."
msgstr "Estimativa de entrega."

#: includes/class-wc-enviofacil-shipping.php:54
msgid "Show estimated delivery"
msgstr "Exibir estimativa de entrega"

#: includes/class-wc-enviofacil-shipping.php:55
msgid "Display the estimated delivery in business day."
msgstr "Exibe o tempo estimado de entrega em dias úteis."

#: includes/class-wc-enviofacil-shipping.php:60
msgid "Enable PAC"
msgstr "Habilitar o PAC"

#: includes/class-wc-enviofacil-shipping.php:62
msgid "Enable PAC as a shipping method."
msgstr "Habilitar o PAC como método de entrega."

#: includes/class-wc-enviofacil-shipping.php:67
msgid "PAC title"
msgstr "Título do PAC"

#: includes/class-wc-enviofacil-shipping.php:69
msgid "Title to be displayed on site."
msgstr "Como o PAC será chamado no site."

#: includes/class-wc-enviofacil-shipping.php:74
msgid "Enable SEDEX"
msgstr "Habilitar o SEDEX"

#: includes/class-wc-enviofacil-shipping.php:76
msgid "Enable SEDEX as a shipping method."
msgstr "Habilitar o SEDEX como método de entrega."

#: includes/class-wc-enviofacil-shipping.php:81
msgid "Título do SEDEX"
msgstr "Título do SEDEX"

#: includes/class-wc-enviofacil-shipping.php:83
msgid "Title to be displayed on site"
msgstr "Como o SEDEX será chamado no site"

#: includes/class-wc-enviofacil-shipping.php:88
msgid "Additional days"
msgstr "Dias adicionais"

#: includes/class-wc-enviofacil-shipping.php:90
msgid "Additional business days to the estimated delivery."
msgstr "Dias úteis adicionados à estimativa de entrega."

#: includes/class-wc-enviofacil-shipping.php:96
msgid "Handling fee"
msgstr "Taxa de manuseio"

#: includes/class-wc-enviofacil-shipping.php:98
msgid "An amount to add to the cost of the shipping method as a fee for handling it."
msgstr "Uma quantia que você deseja adicionar ao custo de envio."
